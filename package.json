{
  "name": "most-test",
  "version": "1.0.0",
  "description": "Unit testing tools for Most.js",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha",
    "lint": "eslint src/*.js",
    "test-dev": "mocha --watch --bail",
    "precompile": "rimraf lib/ && mkdirp lib",
    "compile": "babel -d lib/ src/",
    "build": "npm run compile && npm run lint && npm test",
    "prepublish": "npm run build"
  },
  "keywords": [
    "most",
    "most.js",
    "unit",
    "test",
    "testing"
  ],
  "author": {
    "name": "Nathan Ridley",
    "email": "axefrog@gmail.com"
  },
  "license": "MIT",
  "peerDependencies": {
    "most": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.3",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3"
  }
}
